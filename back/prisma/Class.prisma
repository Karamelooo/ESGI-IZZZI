model Class {
  id            Int     @id @default(autoincrement())
  name          String
  description   String?
  studentCount  Int
  studentEmails String
  institutionId Int

  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

  institution Institution @relation(fields: [institutionId], references: [id])
  subjects    Subject[]

  @@index([institutionId])
}
